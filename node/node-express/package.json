{
  "name": "node-express",
  "version": "1.0.0",
  "description": "boilerplate for REST API server with node and express",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development nodemon ./bin/www",
    "start:debug": "NODE_ENV=development nodemon --inspect ./bin/www",
    "lint": "eslint *.* --fix",
    "format": "prettier --write . && prettier --check . "
  },
  "keywords": [
    "boilerplate",
    "node boilerplate",
    "node express boilerplate",
    "es6"
  ],
  "author": "Abhijeet <ab.progr@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@ab.progr/eslint-config-node-extended": "^1.0.3",
    "@ab.progr/nodemon-config": "^1.0.1",
    "@ab.progr/prettier-config": "^1.0.2"
  },
  "dependencies": {
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "uuid": "^7.0.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  }
}
